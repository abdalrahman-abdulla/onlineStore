    "bootstrap": "^4.5.3",
    "core-js": "^3.6.5",
    "express": "^4.17.1",
    "jquery": "^3.5.1",
    "normalize.css": "^8.0.1",
    "popper.js": "^1.16.1",
    "serve-static": "^1.14.1",
    "sweetalert2": "^10.10.1",
    "vee-validate": "^3.4.3",
    "vue": "^2.6.11",
    "vue-page-transition": "^0.2.2",
    "vue-paypal-checkout": "^3.2.0",
    "vue-router": "^3.2.0",
    "vue2-google-maps": "^0.10.7",
    "vuex": "^3.4.0"
  },
  "devDependencies": {
    "@vue/cli-plugin-babel": "^4.5.0",
    "@vue/cli-plugin-eslint": "^4.5.0",
